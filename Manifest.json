{
    "manifest_version": 3,
    "name": "Block YouTube Shorts",
    "description": "A simple extension to block YouTube Shorts videos.",
    "version": "1.0.5",
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["Content.js"],
        "run_at": "document_end"
      }
    ]
  }